<div class='miniuser-list'>
<center>


<div class="container">
<ul>
	<% @entries.each do |entry| %>
		<div class='miniuser'>
			
				<a href="/entries/<%= entry.id %>"><b><%= entry.title.upcase %></b></a>
				<br>
				<%= entry.post %>
				<br>
				<%= entry.date_posted %>
				<br>
		
		</div>
	<% end %>
</ul>
</div>



</center>
</div>