<center><h1>B L O G H O G</h1></center>
<div class="container">
<ul>
	<% @entries.each do |entry| %>
		<div class='miniuser'>
			<p class='username'>
				<a href="/entries/<%= entry.id %>"><b><%= entry.title.upcase %></b></a>
				<br>
				<%= entry.post %>
				<br>
				<%= entry.date_posted %>
				<br>
			</p>
		</div>
	<% end %>
</ul>
</div>


