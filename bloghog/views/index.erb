<center><h1>B L O G H O G</h1></center>
<div class="container">
<ul>
	<% @entries.each do |entry| %>
		<div class='miniuser'>
			<p class='username'>

				<a href="/entries/<%= entry.id %>"><b><%= entry.title.upcase %></b></a>
				<br>
				<br>
				<%= entry.post %>
				<h5>#<%= entry.tags.join(", #")%></h5>
				<h5>POSTED: <%= entry.date_posted %></h5>
				<br>
			</p>
		</div>
	<% end %>
</ul>
</div>


