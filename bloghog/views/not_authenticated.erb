<center>


<div class="container">
<ul>
	<% @entries.each do |entry| %>
		<div class='miniuser'>
			<p class='username'>
				<a href="/entries/<%= entry.id %>"><b><%= entry.title.upcase %></b></a>
				<br>
				<br>
				<%= entry.post %>
				<h5>#<%= entry.tags.join(", #")%></h5>
				<h5>POSTED: <%= entry.date_posted %> | Written by: <%= entry.user_id %></h5>
			</p>
		</div>
	<% end %>
</ul>
</div>
</center>